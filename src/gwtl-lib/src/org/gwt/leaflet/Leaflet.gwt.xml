<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwtl_lib">
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.json.JSON" />
	<script src="http://cdn.leafletjs.com/leaflet-0.3.1/leaflet-src.js"/>
	<stylesheet src="http://cdn.leafletjs.com/leaflet-0.3.1/leaflet.css"/>
	
	<source path="api"></source>
	<source path="client"></source>
	
	<!-- Leaflet API implementation using dependency injection -->
	<replace-with class="org.gwt.leaflet.client.MapFactoryImpl">
		<when-type-is class="org.gwt.leaflet.api.MapFactory"/>
	</replace-with>
	
</module>
